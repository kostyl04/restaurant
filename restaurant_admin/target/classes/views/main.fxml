<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="rootLayout" prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.durovich.restaurant_admin.controllers.MainController">
   <top>
      <MenuBar prefHeight="29.0" prefWidth="640.0" BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="Admin">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#AddProductBtnClick" text="Add product" />
                  <MenuItem mnemonicParsing="false" onAction="#AddCurrencyBtnClick" text="Add currency" />
                  <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Order">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#MakeOrderBtnClick" text="Make order" />
               </items>
            </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
