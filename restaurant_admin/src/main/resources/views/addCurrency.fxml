<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.durovich.restaurant_admin.controllers.AddCurrencyController">
   <children>
      <TextField fx:id="nameField" layoutX="248.0" layoutY="115.0" prefHeight="25.0" prefWidth="52.0" />
      <Label alignment="CENTER" layoutX="219.0" layoutY="95.0" prefHeight="17.0" prefWidth="111.0" text="Name" textAlignment="CENTER" />
      <Button fx:id="addBTN" layoutX="441.0" layoutY="88.0" mnemonicParsing="false" onAction="#addCurrency" prefHeight="63.0" prefWidth="52.0" text="Add" />
      <TableView fx:id="currencyTable" layoutX="207.0" layoutY="186.0" prefHeight="200.0" prefWidth="226.0">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="136.0" text="Name" />
            <TableColumn fx:id="rateColumn" prefWidth="89.0" text="Rate" />
        </columns>
      </TableView>
      <Button fx:id="deleteBTN" disable="true" layoutX="460.0" layoutY="215.0" mnemonicParsing="false" onAction="#deleteCurrency" prefHeight="50.0" prefWidth="52.0" text="Delete" />
      <Label fx:id="errorMessagesLBL" alignment="CENTER" layoutX="207.0" layoutY="138.0" prefHeight="31.0" prefWidth="226.0" />
      <Label layoutX="209.0" layoutY="112.0" prefHeight="31.0" prefWidth="39.0" text="USD/" />
      <TextField fx:id="rateField" layoutX="330.0" layoutY="115.0" prefHeight="25.0" prefWidth="52.0" />
      <Label alignment="CENTER" layoutX="301.0" layoutY="95.0" prefHeight="17.0" prefWidth="111.0" text="Rate" textAlignment="CENTER" />
   </children>
</AnchorPane>
